{
  "name": "console-streaming-server",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "bin": {
    "console-streaming-server": "main.js"
  },
  "scripts": {
    "start": "qode main.js",
    "debug": "qode --inspect main.js",
    "webpack": "webpack",
    "distinstall": "cd dist && npm init -y && npm install express bufferutil utf-8-validate node-media-server",
    "nodeguipack": "npx nodegui-packer --pack ./dist",
    "copyassets": "cpy assets deploy/win32/build/ConsoleStreamingServer",
    "renameexe": "cd deploy/win32/build/ConsoleStreamingServer && node -e \"require('fs').rename('qode.exe', 'ConsoleStreamingServer.exe', () => {})\"",
    "build": "npm run webpack && npm run distinstall && npm run nodeguipack && npm run copyassets && npm run renameexe"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aioros/console-streaming-server.git"
  },
  "author": "Aioros",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aioros/console-streaming-server/issues"
  },
  "homepage": "https://github.com/Aioros/console-streaming-server#readme",
  "dependencies": {
    "@aiorosdev/dns-proxy-lib": "^1.0.0",
    "@nodegui/nodegui": "^0.62.0",
    "conf": "^12.0.0",
    "internal-ip": "^7.0.0",
    "node-media-server": "^2.6.2",
    "rtmp-client": "^1.5.3"
  },
  "devDependencies": {
    "@aiorosdev/nodegui-packer": "^1.5.1",
    "cpy-cli": "^5.0.0",
    "node-loader": "^2.0.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
